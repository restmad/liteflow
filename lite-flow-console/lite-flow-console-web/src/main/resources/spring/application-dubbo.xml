<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <bean id="springUtils" class="cn.lite.flow.common.utils.SpringUtils"/>

    <dubbo:application name="${dubbo.application.name}">
        <dubbo:parameter key="qos.port" value="${dubbo.qos.port}"/>
    </dubbo:application>
    <dubbo:registry address="${dubbo.registry.address}" file="${dubbo.registry.file}"/>
    <dubbo:protocol name="${dubbo.protocol.name}" port="${dubbo.protocol.port}"/>

    <import resource="application-dubbo-provider.xml"/>
    <import resource="application-dubbo-consumer.xml"/>
</beans>
